#suite test_mul
#tcase case_s21_mul

#test PositiveMultiplication
  // Проверка умножения двух положительных чисел: 5 * 4 должно дать 20
  s21_decimal src1 = {{5, 0, 0, 0}}, src2 = {{4, 0, 0, 0}}, expected = {{20, 0, 0, 0}}, result;
  ck_assert_int_eq(s21_mul(src1, src2, &result), OK);
  ck_assert_mem_eq(&result, &expected, sizeof(s21_decimal));

#test NegativeMultiplication
  // Проверка умножения отрицательного и положительного числа: -3 * 4 должно дать -12
  s21_decimal src1 = {{3, 0, 0, 0x80000000}}, src2 = {{4, 0, 0, 0}}, expected = {{12, 0, 0, 0x80000000}}, result;
  ck_assert_int_eq(s21_mul(src1, src2, &result), OK);
  ck_assert_mem_eq(&result, &expected, sizeof(s21_decimal));

#test OverflowDetection
  // Проверка на обнаружение переполнения при умножении больших чисел
  s21_decimal src1 = {{0, 0, 12345, 0x00030000}}, src2 = {{0, 0, 67890, 0x00040000}}, result;
  ck_assert_int_eq(s21_mul(src1, src2, &result), OK);

#test MultiplicationByZero
  // Проверка умножения на ноль: любое число, умноженное на ноль, должно дать ноль
  s21_decimal src1 = {{0}}, src2 = {{12345, 0, 0, 0}}, result;
  ck_assert_int_eq(s21_mul(src1, src2, &result), OK);
  ck_assert_int_eq(getMantissa(result), 0);

#test ZeroMultiplication
  // Проверка умножения нуля на другое число
  s21_decimal src1 = {{12345, 0, 0, 0x00030000}}, src2 = {{0}}, result;
  ck_assert_int_eq(s21_mul(src1, src2, &result), OK);
  ck_assert_int_eq(getMantissa(result), 0);

#test SimpleMultiplication
  // Тест простого умножения: 2 * 3 должно дать 6
  s21_decimal src1 = {{2, 0, 0, 0}}, src2 = {{3, 0, 0, 0}}, expected = {{6, 0, 0, 0}}, result;
  ck_assert_int_eq(s21_mul(src1, src2, &result), OK);
  ck_assert_mem_eq(&result, &expected, sizeof(s21_decimal));

#test SymmetryCheck
  // Проверка симметрии умножения: 3 * 2 должно дать тот же результат, что и 2 * 3
  s21_decimal src1 = {{3, 0, 0, 0}}, src2 = {{2, 0, 0, 0}}, expected = {{6, 0, 0, 0}}, result;
  ck_assert_int_eq(s21_mul(src1, src2, &result), OK);
  ck_assert_mem_eq(&result, &expected, sizeof(s21_decimal));

#test SmallLargeMultiplication
  // Проверка умножения малого числа на большое
  s21_decimal src1 = {{4, 0, 0, 0}}, src2 = {{97623323, 0, 0, 0}}, expected = {{390493292, 0, 0, 0}}, result;
  ck_assert_int_eq(s21_mul(src1, src2, &result), OK);
  ck_assert_mem_eq(&result, &expected, sizeof(s21_decimal));

#test LargeValuesMultiplication
  // Тест на умножение очень больших значений
  s21_decimal src1 = {{0xFFFFFFFF, 0xFFFFFFFF, 0xFFFFFFFF, 0}}, src2 = {{1, 0, 0, 0}}, expected = {{0xFFFFFFFF, 0xFFFFFFFF, 0xFFFFFFFF, 0}}, result;
  ck_assert_int_eq(s21_mul(src1, src2, &result), OK);
  ck_assert_mem_eq(&result, &expected, sizeof(s21_decimal));

#test RealisticUsageScenario
  // Реалистичный сценарий использования
  s21_decimal src1 = {{2, 0, 0, 0}}, src2 = {{8, 0, 0, 0}}, expected = {{16, 0, 0, 0}}, result;
